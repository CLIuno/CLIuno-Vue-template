{
  "name": "cliuno-vue-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 5000 --host --open http://localhost:5000",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:5000 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 5000' http://localhost:5000 'cypress open --e2e'",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "@vueuse/head": "^1.1.26",
    "@vueuse/integrations": "^10.2.1",
    "@vueuse/math": "^10.2.1",
    "@vueuse/sound": "^2.0.1",
    "animate.css": "^4.1.1",
    "apexcharts": "^3.41.0",
    "axios": "^1.4.0",
    "chart.js": "^4.3.0",
    "pinia": "^2.1.3",
    "primeicons": "^6.0.1",
    "primevue": "^3.32.0",
    "sweetalert2": "^11.7.12",
    "tsparticles": "^2.10.1",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.3.4",
    "vue-apexcharts": "^1.6.2",
    "vue-router": "^4.2.2",
    "vue3-particles": "^2.10.1",
    "vuetify": "^3.3.7"
